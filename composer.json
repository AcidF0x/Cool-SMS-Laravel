{
    "name": "acidf0x/laracool",
    "description": "CoolSMS SDK Wrapper for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Duhui Cho",
            "email": "ic_jdh@me.com"
        }
    ],
    "require": {
        "coolsms/php-sdk": "^2.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "AcidF0x\\LaraCool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AcidF0x\\LaraCool\\CoolSMSServiceProvider"
            ],
            "aliases": {
                "CoolSMS": "AcidF0x\\LaraCool\\Facades\\CoolSMS"
            }
        }
    }
}
